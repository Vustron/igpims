    "db-drop": "bunx drizzle-kit drop",
    "outdated-packages": "bun outdated",
    "update-packages": "bun update --latest",
    "seed-users": "bun run ./src/scripts/seed-users.ts",
    "check-bundle": "ANALYZE=true bun run build",
    "biome-migrate": "bunx --bun biome migrate --write"
